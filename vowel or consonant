#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int j;
    scanf("%lc",&j);
    if((j>='a' && j<='z')||(j>='A' && j<='Z'))
       if(j=='a' || j=='e' || j=='i' || j=='o' || j=='u'|| j=='A' || j=='E' || j=='I' || j=='O' || j=='U')
        printf("Vowel");
       else
        printf("Consonant");
    else
       printf("Not an alphabet");
}
